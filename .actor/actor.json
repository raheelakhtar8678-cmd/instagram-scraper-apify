{
    "actorSpecification": 1,
    "name": "instagram-scraper-premium",
    "title": "Instagram Scraper Pro - Profiles, Posts, Hashtags & Analytics",
    "version": "0.1",
    "dockerfile": "./Dockerfile",
    "input": "./INPUT_SCHEMA.json",
    "storages": {
        "dataset": {
            "actorSpecification": 1,
            "title": "Scraped Instagram Data",
            "views": {
                "dashboard": {
                    "title": "Premium Dashboard",
                    "transformation": {
                        "fields": [
                            "VIEW_PREMIUM_REPORT",
                            "type",
                            "username",
                            "fullName",
                            "followersCount",
                            "postsCount",
                            "isPrivate",
                            "scrapedAt"
                        ]
                    },
                    "display": {
                        "component": "table",
                        "properties": {
                            "VIEW_PREMIUM_REPORT": {
                                "title": "Dashboard",
                                "format": "link"
                            }
                        }
                    }
                },
                "report": {
                    "title": "Direct View",
                    "display": {
                        "component": "html",
                        "properties": {
                            "htmlFieldName": "REPORT_HTML"
                        }
                    }
                }
            }
        }
    }
}